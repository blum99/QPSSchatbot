# Copy these into a local .env.local (never commit real secrets)

# OpenAI Configuration (REQUIRED)
OPENAI_API_KEY=sk-...
OPENAI_ASSISTANT_ID=asst-...

# Frontend Version Selector (REQUIRED)
# Controls which UI layout to display
# v1 = Original simple layout
# v2 = Figma Modern UI
NEXT_PUBLIC_FRONTEND_VERSION=v2

# Optional values if your OpenAI setup uses them
OPENAI_PROJECT=
OPENAI_ORGANIZATION=
OPENAI_BASE_URL=https://api.openai.com/v1
